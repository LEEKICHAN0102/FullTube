extends base
include mixins/video

block content
  nav 
    ul
      if loggedIn
        li
          a(href="/user/logout") 로그아웃 
        li  
          a(href="/profile") #{loggedInUser.username}님 의 프로필 
      else
        li
          a(href="/login") 로그인
        li
          a(href="/join") 회원 가입
      li
        a(href="/video/upload") 영상 업로드
      li 
        a(href="/search") 검색
  each video in videos 
    +video(video)
  else 
    li Video Not exist 