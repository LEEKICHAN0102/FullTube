extends base.pug
include mixins/video.pug

block content 
  div.profile__data
    img(src=`${user.avatarUrl}`).avatar
    h3=user.username
  span.span__style=`${user.username}님이 올린 동영상 목록`
  div.video-grid
      each video in user.videos
          +video(video)
      else
          span.empty__message 올리신 영상이 없습니다.